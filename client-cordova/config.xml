<?xml version='1.0' encoding='utf-8'?>
<widget id="com.dylanbienenstock.gamma" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Gamma</name>
    <description />
    <author email="db@dylanbienenstock.com" href="http://dylanbienenstock.com">
        Dylan Bienenstock
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <hook src="ng-build.js" type="before_prepare" />
    <plugin name="cordova-plugin-dialogs" spec="^2.0.1" />
    <preference name="DisallowOverscroll" value="true" />
    <engine name="android" spec="^7.1.0" />
</widget>
